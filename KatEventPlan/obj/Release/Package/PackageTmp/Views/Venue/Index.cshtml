@model IEnumerable<KatEventPlan.Models.Venue>

@{
    ViewBag.Title = "Venue";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Venue_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Location)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Capacity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Image_Url)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Venue_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Location)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Capacity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Image_Url)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Venue_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Venue_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Venue_Id })
        </td>
    </tr>
}

</table>
